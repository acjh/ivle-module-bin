{
  "manifest_version": 2,

  "name": "IVLE Module Bin",
  "description": "Don't want to see a module? Just bin it away!",
  "version": "0.1.3",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "IVLE Module Bin"
  },

  "content_scripts": [
    {
      "matches": ["https://ivle.nus.edu.sg/v1/workspace.aspx"],
      "js": [
        "jquery-3.1.1.min.js",
        "tooltip.js",
        "popover.js",
        "ivle-module-bin.js",
        "run-once.js"
      ],
      "run_at": "document_end"
    }
  ],

  "permissions": [
    "https://ivle.nus.edu.sg/v1/workspace.aspx"
  ]
}
